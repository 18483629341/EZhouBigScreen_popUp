.new-bg {
    position: relative;
    margin: 2px;
    border: 2px solid #749bdb;
    border: 2px solid #476fb1;
    width: calc(100% - 4px);
    height: calc(100% - 4px);
    background: rgba(95, 121, 169, .2);
}

.new-bg::before {
    content: ' ';
    position: absolute;
    left: -4px;
    top: -4px;
    right: -4px;
    bottom: -4px;
    background: url("../images/4.png") no-repeat left top, url("../images/1.png") no-repeat left bottom, url("../images/3.png") no-repeat right top, url("../images/2.png") no-repeat right bottom;
}

.new-box {
    position: relative;
    z-index: 3;
    width: 100%;
    height: 100%;
}

.new-box::before {
    content: ' ';
    display: block;
    width: 17px;
    height: 14px;
    position: absolute;
    left: 21px;
    top: -2px;
    background: url("../images/9.png") no-repeat left top;
}